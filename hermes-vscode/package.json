{
  "name": "zswe-agent",
  "displayName": "ZSCE Agent",
  "description": "Multi-agent system for automated software development with context engineering",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zswe-agent.startWorkflow",
        "title": "ZSCE Agent: Start Development Workflow",
        "category": "ZSCE Agent"
      },
      {
        "command": "zswe-agent.viewWorkflowStatus",
        "title": "ZSCE Agent: View Workflow Status",
        "category": "ZSCE Agent"
      },
      {
        "command": "zswe-agent.openConstitution",
        "title": "ZSCE Agent: Open Project Constitution",
        "category": "ZSCE Agent"
      }
    ],
    "configuration": {
      "title": "ZSCE Agent",
      "properties": {
        "zsweAgent.apiKey": {
          "type": "string",
          "default": "",
          "description": "Google Gemini API Key for ZSCE Agent"
        },
        "zsweAgent.maxDebateRounds": {
          "type": "number",
          "default": 3,
          "description": "Maximum number of debate rounds between agents"
        },
        "zsweAgent.autoApprove": {
          "type": "boolean",
          "default": false,
          "description": "Auto-approve agent decisions (for testing)"
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "zsweAgentWorkflow",
          "name": "ZSCE Agent Workflow",
          "when": "workspaceHasPythonFiles"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "zswe-agent",
          "title": "ZSCE Agent",
          "icon": "$(lightbulb)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
